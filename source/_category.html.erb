<div class="pl4 mb3">
  <% category_options.each do |option| %>
    <%
        classes = "link-reset db normal "
        classes += option[:current] ? "green b pv1" : "black-50 hover-black-80 pv1"
    %>
    <a href="/docs/<%= option[:href] %>.html" class="<%= classes %>"><%= option[:short_title] %></a>
  <% end %>
</div>
